<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.user.pgdump</string>

    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>pg.sh</string>
        <string>-d</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/raymondleclair/Projects/Home/Toastmasters/Speak-Up-Cambridge/speakup/postgres</string>

    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/opt/local/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
    </dict>

    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>15</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>

    <key>StandardOutPath</key>
    <string>/tmp/com.user.pgdump.out</string>
    <key>StandardErrorPath</key>
    <string>/tmp/com.user.pgdump.err</string>

    <key>RunAtLoad</key>
    <true/>
</dict>
</plist>
